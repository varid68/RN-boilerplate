{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": "./src",
        "alias": {
          "components": "./src/components",
          "actions": "./src/actions",
          "images": "./src/assets/images",
          "configs": "./src/configs",
          "constants": "./src/constants",
          "services": "./src/services",
          "screens": "./src/screens",
          "@redux": "./src/redux"
        },
        "cwd": "packagejson"
      }
    ]
  ]
}